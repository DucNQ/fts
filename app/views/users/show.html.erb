<% # @title = @user.name %>

<div class="row">
  <aside class="span4">
    <section>
      <h1>
        <% #gravatar_for @user %>
        <%= @user.name %>
        <br>
        <%= @user.email %>
      </h1>
    </section>
  </aside>
  <div class="span8">
    <h3>Answer Sheets</h3>
    <%= render 'shared/answer_sheets' %>
  </div>
</div>
